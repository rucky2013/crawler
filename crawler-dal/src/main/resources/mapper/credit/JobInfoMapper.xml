<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.maiya.dal.dao.credit.JobInfoDao">

    <select id="getJobInfoByUserId" resultType="JobInfo">
       <![CDATA[
         SELECT sGuid sUserId,sProvince,sCity FROM hy_jobinfo where sUserId=#{userId}
       ]]>
    </select>

</mapper>